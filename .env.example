# Jenkins URL (Update this with your Jenkins URL)
JENKINS_URL=https://sourabh-jenkins.techis.store

# GitHub Configuration (You'll need to add your GitHub token)
GITHUB_TOKEN=your_github_personal_access_token
WEBHOOK_SECRET=your_webhook_secret_here

# Azure Configuration
AZURE_TENANT_ID=your_tenant_id_here              # Example: xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
AZURE_SUBSCRIPTION_ID=your_subscription_id_here   # Example: xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
ACR_NAME=your_acr_name_here                      # Example: myacrregistry
ACR_LOGIN_SERVER=your_acr_login_server           # Example: myacrregistry.azurecr.io

# Kubernetes Configuration (Using your existing AKS details)
AKS_RESOURCE_GROUP=rg-prod
AKS_CLUSTER_NAME=aks-prod
K8S_NAMESPACE=demo

# Application Configuration
APP_NAME=demo-app
IMAGE_NAME=demo-app
BUILD_NUMBER=${BUILD_NUMBER}
GIT_COMMIT_SHORT=${GIT_COMMIT_SHORT}